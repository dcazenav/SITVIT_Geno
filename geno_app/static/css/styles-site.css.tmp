.M2MSelector 
{
	width:864px;
	margin:0px auto;
	overflow:hidden;
}

.M2MSelector h3
{
	font-weight:bold;
	color:white;
	text-align:center;
	padding: 2px 4px;
	margin:0;
	height:2em;
	line-height:2em;
	font-size: 16px;
}

.M2MWrapper
{
	float:left;
	margin:20px 10px;
	width:250px; 
	background-color:#283e8e;
	border-bottom: 1px solid #333333;	
	border-right: 1px solid #333333;	
	border-left: 1px solid #CCCCCC;	
	border-top: 1px solid #CCCCCC;	
}

.M2MSelectorList 
{ 
	width:248px; 
	height:250px; 
	overflow-y:scroll; 
	overflow-x:hidden; 
	padding:0; 
	margin:0;
	background-color:white;
	border-top: 1px solid #333333;	
	border-left: 1px solid #333333;	
	border-right: 1px solid #CCCCCC;	
	border-bottom: 1px solid #CCCCCC;	
}
.M2MNOT 
{ 
}
.M2MFilterField
{
	margin-left:2em;
	float:right;
}
.M2MSelectorList li 
{
	float:left;
	width:230px;
	background-color:#d8dced;
	margin:1px;
	padding:2px;
	text-align:left;
	list-style-type:none;
	font-weight: bold;
	font-size: 14px;
	color: #324395;
}

th.sortable,
th.sortable-text,
th.sortable-date,
th.sortable-keep,
th.sortable-date-dmy,
th.sortable-numeric,
th.sortable-currency,
th.sortable-sortByTwelveHourTimestamp,
th.sortable-sortIPAddress,
th.sortable-sortEnglishLonghandDateFormat,
th.sortable-sortScientificNotation,
th.sortable-sortImage,
th.sortable-sortFileSize

        {
        cursor:pointer;
        background: #CAE8EA url(http://www.ajaxdaddy.com/web20/table-sort/css/images/bg_header_sortable.jpg) no-repeat;
        padding: 8px 12px 4px 16px;
        }
th.forwardSort
        {
        background:#CAE8EA url(http://www.ajaxdaddy.com/web20/table-sort/css/images/bg_header_down.jpg) no-repeat 0 0;
        }
th.reverseSort
        {
        background:#CAE8EA url(http://www.ajaxdaddy.com/web20/table-sort/css/images/bg_header_up.jpg) no-repeat 0 0;
        }
th.forwardSort a,
th.reverseSort a
        {
        color:#324395;
        outline:0 none;
        }
#ajax_listOfOptions{
  position:absolute;  /* Never change this one */
  width:175px;  /* Width of box */
  height:250px;  /* Height of box */
  overflow:auto;  /* Scrolling features */
  border:1px solid #317082;  /* Dark green border */
  background-color:#FFF;  /* White background color */
  text-align:left;
  font-size:0.9em;
  z-index:100;
}
#ajax_listOfOptions div{  /* General rule for both .optionDiv and .optionDivSelected */
  margin:1px;    
  padding:1px;
  cursor:pointer;
  font-size:0.9em;
}
#ajax_listOfOptions .optionDiv{  /* Div for each item in list */
  
}
#ajax_listOfOptions .optionDivSelected{ /* Selected item in the list */
  background-color:#317082;
  color:#FFF;
}
#ajax_listOfOptions_iframe{
  background-color:#F00;
  position:absolute;
  z-index:5;
}

