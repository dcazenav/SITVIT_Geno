@charset "utf-8";
#page {
	background-color: #FFFFFF;
	padding: 0px;
	width: 1100px;
	margin-top: 0px;
	margin-right: auto;
	margin-bottom: 0px;
	margin-left: auto;
}
#bando {
	background-color: #FFFFFF;
	margin: 0px;
	padding: 0px;
	height: 125px;
	width: 1100px;
	background-image: none;
	background-repeat: no-repeat;
}
#menu {
	margin: 0px;
	padding: 0px;
	height: 30px;
	width: 1100px;
	background-image: url(../images/boutton_bovis.png);
	background-repeat: repeat-x;
}
#menu ul,#widemenu ul {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
}
#menu li, #widemenu li {
	display: inline;
	margin: 0px;
	padding: 0px;
}
#menu li a, #widemenu li a {
	background-image: url(../images/boutton_bovis.png);
	background-repeat: repeat-x;
	display: block;
	color: #FFFFFF;
	clear: none;
	float: left;
	height: 25px;
	margin-top: 0px;
	margin-right: 10px;
	margin-bottom: 0px;
	margin-left: 0px;
	padding-top: 8px;
	padding-right: 5px;
	padding-bottom: 0px;
	padding-left: 5px;
	text-decoration: none;
	font-weight: bold;
	text-transform: uppercase;
	font-size: 14px;
}
#menu li a:hover,#widemenu li a:hover {
	background-image: url(../images/boutton_selected.png);
}
#page #menu .actualy a {
	background-image: url(../images/boutton_selected.png);
	background-repeat: repeat-x;
	display: block;
	color: #FFFFFF;
	clear: none;
	float: left;
	height: 25px;
	margin-top: 0px;
	margin-right: 10px;
	margin-bottom: 0px;
	margin-left: 0px;
	padding-top: 8px;
	padding-right: 5px;
	padding-bottom: 0px;
	padding-left: 5px;
	text-decoration: none;
	font-weight: bold;
	text-transform: uppercase;
	font-size: 14px;}
